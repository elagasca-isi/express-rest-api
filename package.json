{
  "name": "express-rest-api",
  "version": "0.1.0",
  "description": "Express.js REST API",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "format:base": "prettier --parser typescript",
    "format:check": "npm run format:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "format:write": "npm run format:base -- --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run build:live",
    "test": "jest --verbose"
  },
  "keywords": [],
  "author": "Eric Lagasca <develagasca@gmail.com> (https://develagasca.github.io)",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.10",
    "jest": "^26.6.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.0-dev.20201019"
  }
}
